using System;
using System.IO;
using System.Collections.Generic;

namespace ConsoleApp8;


class Program
{

    static void Main(string[] args)
    {
        List<int> lrstPrime = new List<int>();
        lrstPrime.Add(1);
        lrstPrime.Add(3);
        lrstPrime.Add(5);
        lrstPrime.Add(7);

        //For printing a list with indexing --here is number 5--
        Console.WriteLine(lrstPrime[2]);

        //Adding a two new lines in console for better reading and understanding
        Console.WriteLine("\n\n");

        //For printing a whole list
        foreach (int i in lrstPrime)
        {
            Console.WriteLine(i);
        }

    }
}
